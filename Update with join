UPDATE 
    DQSRETAILCUSTPOINTTRANS 
SET 
    DQSRETAILCUSTPOINTTRANS.CUSTACCOUNT = DQSCUSTOMERCARDS.CUSTACCOUNT
FROM 
    DQSRETAILCUSTPOINTTRANS
    JOIN DQSCUSTOMERCARDS 
	ON DQSRETAILCUSTPOINTTRANS.CARDID = DQSCUSTOMERCARDS.CARDID
WHERE 
    DQSRETAILCUSTPOINTTRANS.CUSTACCOUNT = ''
	AND DQSCUSTOMERCARDS.CUSTACCOUNT != '';
